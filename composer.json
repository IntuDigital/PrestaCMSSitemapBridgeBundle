{
    "name": "presta/cms-sitemap-bridge-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS Sitemap Bridge bundle : PrestaSitemapBundle connector for PrestaCMS",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSSitemapBridgeBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alain Flaus",
            "email": "aflaus@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "presta/cms-core-bundle": "1.0.*",
        "presta/sitemap-bundle": "~1.3",
    },
    "autoload": {
        "psr-0": { "Presta\\CMSSitemapBridgeBundle": "" }
    },
    "target-dir": "Presta/CMSSitemapBridgeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
